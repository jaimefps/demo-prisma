  "dependencies": {
    "@prisma/client": "^3.11.0",
    "@types/node": "^17.0.21",
    "apollo-server": "^3.6.4",
    "express": "^4.17.3",
    "firebase-admin": "^10.0.2",
    "graphql": "^16.3.0",
    "graphql-scalars": "^1.15.0",
